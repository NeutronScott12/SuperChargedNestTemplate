{
    "name": "nest-prisma-template",
    "version": "0.0.1",
    "description": "A starter template for nest",
    "author": "Ogneutron",
    "repository": {},
    "private": true,
    "license": "MIT",
    "scripts": {
        "remove:node_modules": "npx rimraf node_modules",
        "prisma:format": "npx prisma format",
        "migrate": "cross-env npx prisma format && npx prisma migrate dev",
        "generate": "npx prisma generate",
        "migrate:generate": "cross-env npm run migrate && npm run generate",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.0",
        "@nestjs/bull": "^0.4.2",
        "@nestjs/common": "^8.2.2",
        "@nestjs/config": "^1.1.0",
        "@nestjs/core": "^8.2.2",
        "@nestjs/graphql": "^9.1.1",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.2.2",
        "@nestjs/throttler": "^2.0.0",
        "@prisma/client": "^3.5.0",
        "apollo-server-cache-redis": "^3.3.0",
        "apollo-server-express": "^3.5.0",
        "bull": "^4.1.1",
        "class-validator": "^0.13.1",
        "cross-env": "^7.0.3",
        "graphql": "15.7.2",
        "graphql-redis-subscriptions": "^2.4.2",
        "graphql-type-json": "^0.3.2",
        "graphql-ws": "^5.5.5",
        "helmet": "^4.6.0",
        "ioredis": "^4.28.0",
        "joi": "^17.4.2",
        "passport": "^0.5.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "ts-morph": "^12.2.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.1.5",
        "@nestjs/schematics": "^8.0.5",
        "@nestjs/testing": "^8.2.2",
        "@types/bull": "^3.15.5",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.11.7",
        "@types/nodemailer": "^6.4.4",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "prettier": "^2.3.2",
        "prisma": "^3.5.0",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.5.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
